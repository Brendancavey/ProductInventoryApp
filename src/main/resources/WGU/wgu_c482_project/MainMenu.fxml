<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WGU.wgu_c482_project.MainController">
   <children>
      <VBox alignment="CENTER" spacing="15.0">
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
         <Label prefHeight="65.0" prefWidth="148.0" text="Main menu" />
          <Button maxWidth="92.0" onAction="#onCreateMenu" text="Create" />
         <Button maxWidth="92.0" mnemonicParsing="false" onAction="#onDisplayMenu" text="Display" />
         <Button maxWidth="92.0" mnemonicParsing="false" onAction="#onActionExit" text="Exit" />
      </VBox>
   </children>
</AnchorPane>
